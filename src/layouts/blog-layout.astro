---
import Layout from "./layout.astro";
import Header from "@/components/Header.astro";

interface Props {
  frontmatter?: {
    title: string;
    spoiler: string;
  };
}

const { frontmatter } = Astro.props;
---

<Layout frontmatter={frontmatter}>
  <slot name="header">
    <Header />
  </slot>
  <slot />
</Layout>
